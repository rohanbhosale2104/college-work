#selection sort
def selection():
    arr=[10,4,1,3,2,1]
    for i in range(len(arr)-1):
        index=i
        for j in range(i+1,len(arr)):
            if(arr[j]<arr[index]):
                index=j
            #swap
            arr[index],arr[i]=arr[i],arr[index]
            for i in range(len(arr)):
                print(arr[i],end=" ")
                return arr
selection()
#output:
1 1 2 3 4 10
                
